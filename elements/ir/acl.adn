internal
{
	acl: Map<string, string>
}

fn init() {
	acl.set('Apple', 'Yes');
	acl.set('Banana', 'No');
}

fn req(rpc_req) {
	match (acl.get(rpc_req.get('name')) == 'Yes') {
		true => {
			send(rpc_req, NET);
		}
		// default includes none or “no”
		false => {
			send(err('acl'), APP);
		}
	}; 
}

fn resp(rpc_resp) {
    send(rpc_resp, APP);	
}