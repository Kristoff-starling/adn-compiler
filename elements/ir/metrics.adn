internal 
{
	record: Map<int, int>
  latency: Vec<int>
}

fn init() {
}

fn req(rpc_req) {
	record.set(rpc_req.get('meta_id'), current_time());
	send(rpc_req, NET);
}

fn resp(rpc_resp) {
  rpc_id := rpc_resp.get('meta_id');
  lat := current_time() - record.get(rpc_id);
  latency.set(latency.size(), lat);
}